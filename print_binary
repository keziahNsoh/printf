#include "main.h"
#include <stdio.h>

/**
 * print_char - Prints a char
 * @list: List of arguments
 * Return: Number of chars printed
 */

void print_binary(unsigned int num)
{
    int i;
    for (i = 31; i >= 0; i--)
    {
        if ((num >> i) & 1)
            printf("1");
        else
            printf("0");
    }
}
